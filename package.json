    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
