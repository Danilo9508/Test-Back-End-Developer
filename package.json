{
  "name": "test-back-end-developer",
  "version": "1.0.0",
  "description": "test back-end developer",
  "main": "index.js",
  "scripts": {
    "test": "set NODE_ENV=test&& jest --verbose",
    "start": "set NODE_ENV=production&& node index.js",
    "dev": "set NODE_ENV=development&& nodemon index.js",
    "migrations:generate": "set NODE_ENV=production&& sequelize-cli migration:generate --name",
    "migrations:run": "set NODE_ENV=production&& sequelize-cli db:migrate",
    "migrations:revert": "set NODE_ENV=production&& sequelize-cli db:migrate:undo",
    "migrations:delete": "set NODE_ENV=production&& sequelize-cli db:migrate:undo:all",
    "seeders:generate": "set NODE_ENV=production&& sequelize-cli seed:generate --name",
    "seeders:run": "set NODE_ENV=production&& sequelize-cli db:seed:all",
    "seeders:revert": "set NODE_ENV=production&& sequelize-cli db:seed:undo",
    "migrationsdev:generate": "set NODE_ENV=development&& sequelize-cli migration:generate --name",
    "migrationsdev:run": "set NODE_ENV=development&& sequelize-cli db:migrate",
    "migrationsdev:revert": "set NODE_ENV=development&& sequelize-cli db:migrate:undo",
    "migrationsdev:delete": "set NODE_ENV=development&& sequelize-cli db:migrate:undo:all",
    "seedersdev:generate": "set NODE_ENV=development&& sequelize-cli seed:generate --name",
    "seedersdev:run": "set NODE_ENV=development&& sequelize-cli db:seed:all",
    "seedersdev:revert": "set NODE_ENV=development&& sequelize-cli db:seed:undo",
    "migrationstest:generate": "set NODE_ENV=test&& sequelize-cli migration:generate --name",
    "migrationstest:run": "set NODE_ENV=test&& sequelize-cli db:migrate",
    "migrationstest:revert": "set NODE_ENV=test&& sequelize-cli db:migrate:undo",
    "migrationstest:delete": "set NODE_ENV=test&& sequelize-cli db:migrate:undo:all",
    "seederstest:generate": "set NODE_ENV=test&& sequelize-cli seed:generate --name",
    "seederstest:run": "set NODE_ENV=test&& sequelize-cli db:seed:all",
    "seederstest:revert": "set NODE_ENV=test&& sequelize-cli db:seed:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo9508/Test-Back-End-Developer.git"
